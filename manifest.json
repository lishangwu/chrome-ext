{
  "manifest_version": 2,
  "name": "chrome-ext",
  "version": "1.0",
  "description": "A hello world extension!",
  
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "background":
	{
    "scripts": [
      "jquery-3.1.0.min.js",
      "eventPage.js"
    ],
    "persistent": true
  },

  "content_scripts":[
    {
        "matches": ["<all_urls>"],
        "js": [
          "jquery-3.1.0.min.js",
          "md5.min.js",
          "md5.js",
          "tjs.browser.js",
          "content.js"
        ]
    }
],
  
  "permissions": [
    "contextMenus", 
		"tabs", 
		"notifications", 
		"webRequest",
		"webRequestBlocking",
		"storage", 
		"http://*/*",
		"https://*/*"
  ]
}