{
    "manifest_version": 2,
    "name": "chrome-ext",
    "version": "1.0",
    "description": "A hello world extension!",
    "icons":{
      "128":"mountietocat.png",
      "48":"mountietocat.png",
      "16":"mountietocat.png"
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "mountietocat.png",
      "default_popup": "popup.html"
    }
  }