{
  "manifest_version": 2,
  "name": "chrome-ext",
  "version": "1.0",
  "description": "A hello world extension!",
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "<all_urls>"
  ]
}